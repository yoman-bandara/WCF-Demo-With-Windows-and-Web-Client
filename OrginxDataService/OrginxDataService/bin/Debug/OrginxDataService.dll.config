<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
    </configSections>
    <connectionStrings>
        <add name="OrginxDataService.Properties.Settings.OriginXDBConnectionString"
            connectionString="Data Source=RANGANATH;Initial Catalog=OriginXDB;Integrated Security=True"
            providerName="System.Data.SqlClient" />
        <add name="OrginxDataService.Properties.Settings.OriginXDBConnectionString1"
            connectionString="Data Source=YOMAN;Initial Catalog=OriginXDB;Integrated Security=True"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
                    <serviceDebug includeExceptionDetailInFaults="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service name="OrginxDataService.CountryBranchService">
                <endpoint address="" binding="basicHttpBinding" contract="OrginxDataService.ICountryBranchService">
                    <identity>
                        <dns value="localhost" />
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8733/Design_Time_Addresses/OrginxDataService/CountryBranchService/" />
                    </baseAddresses>
                </host>
            </service>
            <service name="OrginxDataService.EmailService">
                <endpoint address="" binding="basicHttpBinding" contract="OrginxDataService.IEmailService">
                    <identity>
                        <dns value="localhost" />
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8733/Design_Time_Addresses/OrginxDataService/EmailService/" />
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
</configuration>
